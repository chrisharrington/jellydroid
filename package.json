{
  "name": "jellydroid",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "bunx expo start",
    "build:dev": "bunx dotenv -- eas build --profile development --platform android --local --output dev-build.apk && qrcp dev-build.apk",
    "test": "bunx jest",
    "type-check": "bunx tsc --noEmit"
